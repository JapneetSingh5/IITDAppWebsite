        "@material-ui/icons": "^4.9.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "@trendmicro/react-sidenav": "^0.5.0",
        "animejs": "^3.2.0",
        "clsx": "^1.1.0",
        "feather-icons": "^4.28.0",
        "mdbreact": "^4.26.1",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-feather": "^2.0.8",
        "react-grid-layout": "^0.18.3",
        "react-helmet": "^6.0.0",
        "react-i18next": "^11.4.0",
        "react-leaflet": "^2.7.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.1",
        "react-share": "^4.1.0",
        "react-social-icons": "^4.1.0",
        "react-use": "^14.2.0",
        "rsuite": "^4.6.0",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.88.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "eslint-test": "eslint './**/*.js' './**/*.jsx'",
        "eslint-fix": "eslint --fix './**/*.js' './**/*.jsx'",
        "prettier-test": "prettier --check './**/*.js' './**/*.jsx' './**/*.css'",
        "prettier-fix": "prettier --write './**/*.js' './**/*.jsx' './**/*.css'",
        "lint-tests": "npm run eslint-test;npm run prettier-test",
        "lint-fixes": "npm run eslint-fix;npm run prettier-fix",
        "css-build": "node-sass --omit-source-map-url src/App.scss src/App.css",
        "watcher": "onchange \"src/App.scss\" --npm run css-build "
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "material-ui": "^0.20.2",
        "node-sass": "^4.14.1",
        "prettier": "^2.0.5",
        "react-social-icons": "^4.1.0"
    }
}
